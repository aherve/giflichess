{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "aherve/giflichess:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/tmp",
      "ContainerDirectory": "/tmp"
    }
  ],
  "Logging": "/var/log/nginx",
  "Command": "serve -p 8080"
}
